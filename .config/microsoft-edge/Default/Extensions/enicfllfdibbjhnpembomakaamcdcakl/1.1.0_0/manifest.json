{
   "action": {
      "default_popup": "popup.html",
      "default_title": "Catppuccin for Web File Explorer Icons"
   },
   "background": {
      "service_worker": "background.js"
   },
   "content_scripts": [ {
      "js": [ "content-scripts/content.js" ],
      "matches": [ "*://bitbucket.org/*", "*://codeberg.org/*", "*://gitea.com/*", "*://github.com/*", "*://gitlab.com/*", "*://tangled.sh/*" ],
      "run_at": "document_start"
   } ],
   "description": "Soothing pastel icons for web file explorers",
   "icons": {
      "128": "icons/128.png",
      "16": "icons/16.png",
      "32": "icons/32.png",
      "48": "icons/48.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "Catppuccin for Web File Explorer Icons",
   "optional_host_permissions": [ "*://*/*" ],
   "permissions": [ "storage", "contextMenus", "activeTab", "scripting" ],
   "update_url": "https://edge.microsoft.com/extensionwebstorebase/v1/crx",
   "version": "1.1.0"
}
